<table class="table table-striped table-advance table-hover" id="tableIssueTasks">
    <tbody id="IssueTasksTableBody">
        {% for task in item.tasks %}
            <tr>
                <td>{{ task.pipeline.board.name }} - {{ task.pipeline.name }}</td>
                <td class="w-25">
                    <div class="btn-group">
                        {#
                        <a class="btn btn-primary btnIssueTask"
                           href="{{ path('vsorg_project_issue_task_edit', {'issueId': item.id, 'taskId': task.id}) }}"
                           title="{{ 'vs_application.template.items_index_row_action_edit' | trans( {},'VSApplicationBundle' ) }}"   
                        >
                            <i class="fas fa-edit"></i>
                        </a>
                        <a class="btn btn-danger btnDeleteResource"
                            href="{{ path('vsorg_kanbanboard_task_delete', {'id': task.id}) }}"
                            data-csrfToken="{{ csrf_token( task.id ) }}"
                            data-redirectUrl="{{ path( 'vsorg_project_issues_update', {'id': item.id} ) }}"
                            title="{{ 'vs_application.template.items_index_row_action_delete' | trans( {},'VSApplicationBundle' ) }}"
                        >
                            <i class="icon_close_alt2"></i>
                        </a>
                        #}
                    </div>
                </td>
            </tr>
        {% endfor %}
    </tbody>
</table>