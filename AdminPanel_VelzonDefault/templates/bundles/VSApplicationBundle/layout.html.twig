<!doctype html>
<html lang="{{ app.request.locale|slice(0, 2) }}"
    data-layout="vertical"
    data-topbar="light"
    data-sidebar="dark"
    data-sidebar-size="lg"
    data-sidebar-image="none"
    data-preloader="disable"
>
	<head>
        <title>{% block title %}.:: {{ applicationTitle | trans( {},'VSApplicationBundle' ) }}{% endblock %}</title>

        {% block head_meta %}
            <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
            {% if applicationIcon is defined %}
                <link rel="icon" type="image/svg+xml" href="{{ asset( applicationIcon ) }}" />
            {% endif %}
        {% endblock %}

        {% block head_styles %}
            {{ encore_entry_link_tags( 'css/app', null, 'VelzonTheme' ) }}
        {% endblock %}

        {% block head_scripts %}
            {{ encore_entry_script_tags( 'js/layout', null, 'VelzonTheme' ) }}
        {% endblock %}
    </head>
    <body>
        <!-- Begin page -->
        <div id="layout-wrapper">
            {% include '@VSApplication/Layout/top.html.twig' %}
            
            <!-- ========== App Menu ========== -->
            <div class="app-menu navbar-menu">
                {% include '@VSApplication/Layout/side.html.twig' %}
            </div>
            <!-- Left Sidebar End -->
            
            <!-- Vertical Overlay-->
            <div class="vertical-overlay"></div>
    
            <!-- ============================================================== -->
            <!-- Start right Content here -->
            <!-- ============================================================== -->
            <div class="main-content">
    
                <div class="page-content">
                    <div class="container-fluid">
                    
                        <!-- start page title -->
                        <div class="row">
                            <div class="col-12">
                                <div class="page-title-box d-sm-flex align-items-center justify-content-between">
                                    <h4 class="mb-sm-0">{% block pageTitle %}{% endblock %}</h4>
                        
                                    <div class="page-title-right">
                                        {{ knp_menu_render('breadcrumbs', {'template': '@VSApplication/Menu/breadcrumb.html.twig'}) }}
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!-- end page title -->
                    
                        {% block content %}{% endblock %}
                    </div>
                </div>
                
                <footer class="footer">
                    <div class="container-fluid">
                        {% include '@VSApplication/Layout/footer.html.twig' %}
                    </div>
                </footer>
            </div>
        </div>
        
        <!-- Modal Dialogs -->
        {% block modals %}
            {% include '@VSApplication/Modal/remove_notification.html.twig' %}
        {% endblock %}
        
        <!-- Optional JavaScript -->
        {% block body_scripts %}
            {{ encore_entry_script_tags( 'js/app', null, 'VelzonTheme' ) }}
        {% endblock %}
    </body>
</html>
                	